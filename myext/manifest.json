{
  "name": "StudyAssist",
  "version": "1.0.0",
  "description": "Study time management assistant for students- organize your time and be aware of your productivness.",
  "manifest_version": 2,
  "key": "",
  "icons": {
    "48": "./images/time-icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "idle",
    "identity",
    "identity.email",
    "storage",
    "activeTab",
    "webNavigation"
  ],
  "oauth2": {
    "client_id": "154483090163-joiepoq9j1mbede019eap3m8tcfaju8f.apps.googleusercontent.com",
    "scopes": ["profile email", "https://www.googleapis.com/auth/contacts"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net https://rawgit.com; object-src 'self'",
  "web_accessible_resources": [
    "./sounds/alarm-pause.mp3",
    "./sounds/alarm-start.mp3",
    "./sounds/alarm-sound.mp3"
  ]
}
